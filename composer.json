{
    "name": "fifthgate/gmaps",
    "description": "A simple Google Maps system for Laravel 7+",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sam Baynham",
            "email": "sam@fifthgate.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Fifthgate\\GMaps\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fifthgate\\GMaps\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "fifthgate/objectivity-core": "^0.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.3",
        "orchestra/testbench": "7.0.x-dev"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:fifthgate/objectivity-core.git"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Fifthgate\\GMaps\\GMapsServiceProvider"
            ]
        }
    }
}
